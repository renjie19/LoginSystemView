<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gania.jonh.employee.controller.CreateEmployeeController">
   <children>
      <GridPane layoutX="167.0" layoutY="99.0" prefHeight="202.0" prefWidth="267.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="254.0" minWidth="10.0" prefWidth="210.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="34.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="35.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="employeeNameField" promptText="Name" />
            <TextField fx:id="ageField" promptText="Age" GridPane.rowIndex="1" />
            <TextField fx:id="positionField" promptText="Position" GridPane.rowIndex="3" />
            <TextField fx:id="addressField" promptText="Address" GridPane.rowIndex="2" />
            <TextField fx:id="licenseField" promptText="License Number" GridPane.rowIndex="4" />
         </children>
      </GridPane>
      <ButtonBar layoutX="386.0" layoutY="346.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#onEmployeeSaveClick" text="SAVE" />
            <Button mnemonicParsing="false" onAction="#onEmployeeClearClick" text="CLEAR" />
        </buttons>
      </ButtonBar>
      <Label layoutX="248.0" layoutY="65.0" text="EMPLOYEE DETAILS" />
   </children>
</AnchorPane>
